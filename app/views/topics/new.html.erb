<% provide(:title, 'New topic') %>
<h1> New Topic </h1>
<div class="row">
  <div class="span6 offset3">
    
    <%= form_for(@topic) do |f| %>
      <%= render 'topic' , f: f %>
    
      <%= f.submit "Create", class: "btn btn-large btn-primary" %>
  <% end %>
  
  <%= render 'back' %>
  </div>
</div>